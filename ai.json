{
  "log": {
    "level": "warn",
    "timestamp": true
  },
  "dns": {
    "servers": [
      {
        "tag": "google",
        "address": "https://dns.google/dns-query",
        "address_resolver": "system",
        "detour": "direct"
      },
      {
        "tag": "ali",
        "address": "https://dns.alidns.com/dns-query",
        "address_resolver": "system",
        "detour": "direct"
      },
      {
        "tag": "cloudflare",
        "address": "https://1.1.1.1/dns-query",
        "address_resolver": "system",
        "detour": "direct"
      },
      {
        "tag": "quad9",
        "address": "https://9.9.9.9/dns-query",
        "address_resolver": "system",
        "detour": "direct"
      }
    ],
    "rules": [
      { "outbound": "direct", "server": "google", "geosite": "cn" },
      { "outbound": "direct", "server": "ali", "geosite": "cn" },
      { "outbound": "direct", "server": "cloudflare", "geosite": "geolocation-!cn" },
      { "outbound": "direct", "server": "quad9", "geosite": "geolocation-!cn" }
    ],
    "strategy": "prefer_ipv4",
    "disable_cache": false,
    "disable_expire": false,
    "independent_cache": true,
    "final": "cloudflare",
    "fakeip": {
      "enabled": true,
      "inet4_range": "198.18.0.0/16",
      "inet6_range": "fc00::/18"
    }
  },
  "route": {
    "rule_set": [
      {
        "tag": "geoip-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://raw.githubusercontent.com/Loyalsoldier/geoip/release/CN.ipset",
        "update_interval": "24h"
      },
      {
        "tag": "geosite-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://raw.githubusercontent.com/Loyalsoldier/domain-list-custom/release/cn.txt",
        "update_interval": "24h"
      }
    ],
    "rules": [
      { "rule_set": "geoip-cn", "outbound": "direct" },
      { "rule_set": "geosite-cn", "outbound": "direct" }
    ],
    "final": "proxy"
  },
  "outbounds": [
    {
      "tag": "direct",
      "type": "direct"
    },
    {
      "tag": "block",
      "type": "block"
    },
    {
      "tag": "COMPATIBLE",
      "type": "direct"
    },
    {
      "tag": "proxy",
      "type": "selector",
      "outbounds": ["hk", "tw", "jp", "sg", "us", "COMPATIBLE"]
    },
    {
      "tag": "hk",
      "type": "urltest",
      "outbounds": ["COMPATIBLE"],
      "url": "https://cp.cloudflare.com",
      "interval": "5m",
      "tolerance": 50
    },
    {
      "tag": "tw",
      "type": "urltest",
      "outbounds": ["COMPATIBLE"],
      "url": "https://cp.cloudflare.com",
      "interval": "5m",
      "tolerance": 50
    },
    {
      "tag": "jp",
      "type": "urltest",
      "outbounds": ["COMPATIBLE"],
      "url": "https://cp.cloudflare.com",
      "interval": "5m",
      "tolerance": 50
    },
    {
      "tag": "sg",
      "type": "urltest",
      "outbounds": ["COMPATIBLE"],
      "url": "https://cp.cloudflare.com",
      "interval": "5m",
      "tolerance": 50
    },
    {
      "tag": "us",
      "type": "urltest",
      "outbounds": ["COMPATIBLE"],
      "url": "https://cp.cloudflare.com",
      "interval": "5m",
      "tolerance": 50
    }
  ]
}
